<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.example.org/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://www.example.org/">

	<xsd:include schemaLocation="common.xsd"/>
    <xsd:element name="TacPackage" type="TacPackageType"/>

	<xsd:complexType name="TacPackageType">
		<xsd:annotation>
			<xsd:documentation>Пакет информационного обмена между ФБУ, ОВК и изготовителем карт тахографа
				карт</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="MessageInfo" type="MessageInfoType" minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="PackageInfo" type="PackageInfoType"
				minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Информация о пакете
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="Order" type="OrderType" minOccurs="1"
					maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>
							Заявление на выдачу карт тахографа:
							Отсылается из ОВК изготовителю карт, при
							этом формируется уникальный номер заявления,
							затем изготовителем после проверки данных на
							возможность изготовления, отсылается в ФБУ
							для формирования номера карты
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Order_v2" type="Order_v2Type"
					minOccurs="1" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>
							Заявление на выдачу карт тахографа нового
							образца: Отсылается из ОВК изготовителю
							карт, при этом формируется уникальный номер
							заявления, затем изготовителем после
							проверки данных на возможность изготовления,
							отсылается в ФБУ для формирования номера
							карты
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="CardStatusChangeNotification"
					type="CardStatusChangeNotificationType" minOccurs="1"
					maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>
							Уведомление о изменении статусов карт
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="TacPackageStatusChangeNotification"
					type="TacPackageStatusChangeNotificationType" minOccurs="1"
					maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
							Уведомление о изменении статуса пакета
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="OrderStatusChangeNotification"
					type="OrderStatusChangeNotificationType" minOccurs="1"
					maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>
							Уведомление о изменении статусов заявлений:
							Отсылается изготовителем в ОВК в случае
							отказа на этапе проверки данных либо отказа
							ФБУ, при этом заполняется причина отказа Из
							ФБУ изготовителю карт в случае присвения
							номеров карт, при этом заполняется
							информация о номерах картах и сроках
							действия карт Из ФБУ изготовителю карт в
							случае отказа на этапе проверки данных, при
							этом заполняется причина отказа От
							изготовителя в ФБУ и ОВК в случае выпуска
							карт, при этом заполняется вся информация о
							картах
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="MessageConfirmation" type="MessageConfirmationType" minOccurs="1" maxOccurs="1"></xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="OrderType">
		<xsd:annotation>
			<xsd:documentation>Заявление на выдачу карт тахографа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OrderNumber" type="OrderNumberType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Номер заявления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="OrderDate" type="xsd:date" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Дата подачи заявления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="OrderCardType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Тип карты:
						D - карта водителя
						M - карта мастерской
						P - карта предприятия
						K - карта контролера </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="D"/>
						<xsd:enumeration value="M"/>
						<xsd:enumeration value="P"/>
						<xsd:enumeration value="K"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="OrderCardCount" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Количество карт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:choice>
				<xsd:element name="PersonOrder" type="PersonOrderType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation> Данные, если заявитель не компания </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="OrgOrder" type="OrgOrderType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation> Данные, если заявитель компания </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="SupervisorOrder" type="SupervisorOrderType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation> Данные, если заявитель котролирующая организация </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="Attachment" type="AttachmentsType" minOccurs="0"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation> Вложения за исключением фотографии/изображения подписи
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:complexType name="Order_v2Type">
        <xsd:annotation>
            <xsd:documentation>Заявление на выдачу карт тахографа</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
        	<xsd:element name="OrderNumber" type="OrderNumberType"
        		minOccurs="1" maxOccurs="1">
        		<xsd:annotation>
        			<xsd:documentation>
        				Номер заявления
        			</xsd:documentation>
        		</xsd:annotation>
        	</xsd:element>

        	<xsd:element name="OrderDate" type="xsd:date" minOccurs="1"
        		maxOccurs="1">
        		<xsd:annotation>
        			<xsd:documentation>
        				Дата подачи заявления
        			</xsd:documentation>
        		</xsd:annotation>
        	</xsd:element>

        	<xsd:element name="OrderCardType" minOccurs="1"
        		maxOccurs="1">
        		<xsd:annotation>
        			<xsd:documentation>
        				Тип карты: D - карта водителя M - карта
        				мастерской P - карта предприятия K - карта
        				контролера
        			</xsd:documentation>
        		</xsd:annotation>
        		<xsd:simpleType>
        			<xsd:restriction base="xsd:string">
        				<xsd:enumeration value="D" />
        				<xsd:enumeration value="M" />
        				<xsd:enumeration value="P" />
        				<xsd:enumeration value="K" />
        			</xsd:restriction>
        		</xsd:simpleType>
        	</xsd:element>

        	<xsd:element name="OrderCardCount" type="xsd:int"
        		minOccurs="1" maxOccurs="1">
        		<xsd:annotation>
        			<xsd:documentation>
        				Количество карт
        			</xsd:documentation>
        		</xsd:annotation>
        	</xsd:element>

        	<xsd:element name="OrderReason" minOccurs="1"
        		maxOccurs="1">
        		<xsd:simpleType>
        			<xsd:restriction base="xsd:string">
        				<xsd:enumeration value="create"></xsd:enumeration>
        				<xsd:enumeration value="change"></xsd:enumeration>
        				<xsd:enumeration value="refresh"></xsd:enumeration>
        				<xsd:enumeration value="newpd"></xsd:enumeration>
        			</xsd:restriction>
        		</xsd:simpleType>
        	</xsd:element>
        	<xsd:choice>
        		<xsd:element name="PersonOrder"
        			type="PersonOrder_v2Type" minOccurs="0" maxOccurs="1">
        			<xsd:annotation>
        				<xsd:documentation>
        					Данные, если заявитель не компания
        				</xsd:documentation>
        			</xsd:annotation>
        		</xsd:element>
        		<xsd:element name="CompanyOrder" type="CompanyOrderType"
        			minOccurs="0" maxOccurs="1">
        			<xsd:annotation>
        				<xsd:documentation>
        					Данные, если заявитель компания
        				</xsd:documentation>
        			</xsd:annotation>
        		</xsd:element>
        		<xsd:element name="WorkshopOrder"
        			type="WorkshopOrderType" minOccurs="0" maxOccurs="1">
        			<xsd:annotation>
        				<xsd:documentation>
        					Данные, если заявитель мастерская
        				</xsd:documentation>
        			</xsd:annotation>
        		</xsd:element>
        		<xsd:element name="SupervisorOrder" 
        			type="SupervisorOrderType" minOccurs="0" maxOccurs="1">
        			<xsd:annotation>
        				<xsd:documentation>
        					Данные, если заявитель котролирующая
        					организация
        				</xsd:documentation>
        			</xsd:annotation>
        		</xsd:element>
        	</xsd:choice>
        	<xsd:element name="Attachment" type="Attachments_v2Type"
        		minOccurs="0" maxOccurs="unbounded">
        		<xsd:annotation>
        			<xsd:documentation>
        				Вложения за исключением фотографии/изображения
        				подписи
        			</xsd:documentation>
        		</xsd:annotation>
        	</xsd:element>
        </xsd:sequence>
    </xsd:complexType>

	<xsd:complexType name="CardStatusChangeNotificationType">
		<xsd:annotation>
			<xsd:documentation> Уведомление о изменении статуса карты </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CardNumber" type="CardNumberType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Номер карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="CardStatus" minOccurs="1" maxOccurs="1">
				<xsd:annotation>

                    <!--
                        Статусы карты в соотвествии с регламентом:

                            Статус              Комментарий                                                                 Идентификатор
                        =====================================================================================================================================================
                        "0. Зарезервирована"    Начальное состояние в перечне (БД)                                          [не является предметом уведомлений]
                        "1. Учтена"             Для КТ присвоен номер и срок действия, выпущен сертификат.                  cardReserved
                        "2. Не изготавливалась" Производство карты отмененено по каким-либо причинам                        cardSuspended
                        "3. Выпущена"           От Производителя получено сообщение о выпущенной карте тахографа.           cardPersonalized
                        "4. Активна"            От Производителя(?) получено сообщение о передаче КТ получателю карты.      cardGivenToCardHolder
                        "5. Утрачена"           От Производителя получен запрос на присвоение номера/срока                  cardLost
                                                действия КТ и выпуск сертификата по заявлению со справкой об утере/краже.
                        "6. Недействительна"    От Производителя получено сообщение об обнаружении неисправимого брака,     cardVoid
                                                деактивации текущей карты или истек срок действия КТ
                                                (автоматическое изменение статуса).    
                        "7. Утилизирована"      От Производителя получено сообщение об утилизации КТ.                       cardRecycled

                        ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                        Примечания:
                        Пространство статусов карты уже, чем пространство статусов заявлений, например - у карты отсутствует статус "Отгружена" и пр. 
                        Предлагается: при просмотре детальной информации о карте дополнять ее статус статусом заявления, например:
                            Статус карты:       зарезервирована
                            Статус заявления:   отправлено в УЦ
                        и т.п.

                        ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                        статусы, не вошедшие в новый регламент, но существующие в текущей системе (mapping с новое пространство состояний):
                        ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
						SendPersonalisation         отправлена на изготовление              cardReserved
						Personalized                изготовлена                             cardPersonalized
						NotPersonalized             Не изготовлена                          cardSuspended
						SendAgent                   Отправлена в ОВК                            [*obsolete status*]
						SendCustomer                Отправлена(выдана) потребителю          cardGivenToCardHolder
						SendUtilized                Отправлена на утилизацию                    [*obsolete status*]
						Utilized                    Утилизирована                           cardRecycled
						Recall                      Отозвана                                cardVoid
						Lost                        Утеряна                                 cardLost
                        =====================================================================================================================================================

                    -->
                    <xsd:documentation>
                        Примечание: в (скобках) указано прежнее значение статуса.

                        cardReserved (SendPersonalisation)
                        "1. Учтена"

                        cardSuspended (NotPersonalized)
                        "2. Не изготавливалась"

                        cardPersonalized (Personalized)
                        "3. Выпущена"

                        cardGivenToCardHolder (SendCustomer)
                        "4. Активна"

                        cardLost (Lost)
                        "5. Утрачена"

                        cardVoid (Recall)
                        "6. Недействительна"

                        cardRecycled (Utilized)
                        "7. Утилизирована"
                    </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
                        <xsd:enumeration value="cardReserved"/>
                        <xsd:enumeration value="cardSuspended"/>
                        <xsd:enumeration value="cardPersonalized"/>
                        <xsd:enumeration value="cardGivenToCardHolder"/>
                        <xsd:enumeration value="cardLost"/>
                        <xsd:enumeration value="cardVoid"/>
                        <xsd:enumeration value="cardRecycled"/>

                        <!-- obsolete but still supported statuses -->

                        <xsd:enumeration value="SendPersonalisation"/>
						<xsd:enumeration value="Personalized"/>
						<xsd:enumeration value="NotPersonalized"/>
                        <!-- <xsd:enumeration value="SendAgent"/> -->
						<xsd:enumeration value="SendCustomer"/>
						<!-- <xsd:enumeration value="SendUtilized"/> -->
						<xsd:enumeration value="Utilized"/>
						<xsd:enumeration value="Recall"/>
						<xsd:enumeration value="Lost"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CardStatusChangeDate" type="xsd:date" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Дата изменения статуса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StateChangeReason" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation> Причина изменения статуса </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TacPackageStatusChangeNotificationType">
		<xsd:annotation>
			<xsd:documentation> Уведомление о изменении статуса пакета </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<xsd:element name="TacPackageStatusChangeDate" type="xsd:date" minOccurs="1"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Дата изменения статуса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="TacPackageStatus" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation> Новый статус пакета:
						FBUReject - отклонено ФБУ, при этом заполняется причина отклонения
						CardManReject - отклонено производителем карт
						DuplicatePackage - пакет с таким номером уже существует
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="FBUReject"/>
						<xsd:enumeration value="CardManReject"/>
						<xsd:enumeration value="DuplicatePackage"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="StateChangeReason" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation> Причина изменения статуса </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="OrderStatusChangeNotificationType">
		<xsd:annotation>
			<xsd:documentation> Уведомление о изменении статуса заявления </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OrderNumber" type="OrderNumberType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Номер заявления
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="OrderData" type="xsd:date" maxOccurs="1"/>
			<xsd:element name="OrderStatusChangeDate" type="xsd:date" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Дата изменения статуса
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="OrderStatus" maxOccurs="1">
				<xsd:annotation>
                    <!--
                        Статусы заявлений в соотвествии с регламентом:

                            Статус                                                  Инициатор статуса   Уведомления         Идентификатор
                        =====================================================================================================================================================
                        "1. Создана"                                                ОВК                 ОВК(internal)        (appOvkCreated)
                        "2. Проверена"                                              ОВК                 ОВК(internal)        (appOvkCheckPointPassed)
                        "3. Отправлена производителю"                               ОВК                 ОВК(internal)        (appOvkSentToMfr)
                        "4. Получена производителем"                                Атлас               Атлас->ОВК          appMfrReceivedFromOvk
                        "5. Отклонена (производителем или ФБУ)"                     Атлас/ФБУ           Атлас->ОВК          appMfrRejected
                        "6. В работе (у производителя)"                             Атлас               Атлас->ОВК          appMfrProcessing
                        "7. Ожидает оплаты (у производителя)"                       Атлас               Атлас->ОВК          appMfrPaymentCheckPoint
                        "8. Отправлена в ФБУ"                                       Атлас               Атлас->ОВК          appMfrSentToFbu
                        "9. Получена в ФБУ"                                         ФБУ                 Атлас->ОВК          appFbuReceived
                        "10. Взята в работу в ФБУ"                                  ФБУ                 ФБУ->Атлас->ОВК     appFbuProcessing
                        "11. Исполнена ФБУ"                                         ФБУ                 Атлас->ОВК          appFbuCompleted
                        "12. Отправлена в УЦ"                                       Атлас               Атлас->АК           appMfrSentToCA
                        "13. Получена в УЦ"                                         АК                  АК->Атлас->ОВК      appCAReceivedFromMfr
                        "14. Исполнена УЦ"                                          АК                  АК->Атлас->ОВК      appCADone
                        "15. Отклонена УЦ"                                          АК                  АК->Атлас->ОВК      appCARejected
                        "16. Произведена"                                           Атлас               Атлас->ОВК          appMfrPersonalized / appMfrPartiallyPersonalized
                        "17. Отгружена"                                             Атлас               Атлас->ОВК          appMfrShippedToOvk
                        "18. Исполнена"                                             Атлас               Атлас->ОВК          appMfrDone

                        ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                        статусы, не вошедшие в новый регламент, но существующие в текущей системе:
                        ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                        а) FBUReject                        отвергнуто ФБУ, при этом заполняется причина                    appFbuRejected
                                                            отклонения (в регламенте: общее отклоенение для Атласа/ФБУ)
						б) FBUDuplicateOrder                заявление с таким номером уже зарегистрировано в ФБУ            appFbuRejectedDuplicate
                        в) CardManDuplicateOrder            заявление с таким номером уже зарегистрировано у                appMfrRejectedDuplicate
                                                            производителя карт

                        =====================================================================================================================================================
                    -->
					<xsd:documentation>
                        Примечание: в (скобках) указано прежнее значение статуса.

                        appMfrReceivedFromOvk:
                        принято Производителем в работу;
                        [app::"4. Получена производителем"]

                        appMfrRejected (NotPersonalized):
                        отвергнуто (возможно задержано) изготовителем на этапе проверки данных, при этом заполняется причина отклонения FBUDuplicateOrder - заявление с таким
                        номером уже зарегистрировано в ФБУ CardManDuplicateOrder - заявление с таким номером уже зарегистрировано у производителя карт;
                        [app::"5. Отклонена (производителем или ФБУ)"]

                        appMfrRejectedDuplicate (CardManDuplicateOrder):
                        [app::"в) CardManDuplicateOrder"]

                        appMfrProcessing:
                        Пакет, в который входит заявка, прошел финансовый контроль у Производителя;
                        [app::"6. В работе (у производителя)"]

                        appMfrPaymentCheckPoint:
                        От Производителя получена информация о не прохождении финансового контроля пакетом заявок, в который входит заявка;
                        [app::"7. Ожидает оплаты (у производителя)"]

                        appMfrSentToFbu (SendFBU):
                        получена информация об отправке запроса (пакета заявок) в ФБУ на регистрацию карт тахографа и выпуск сертификатов.
                        [app::"8. Отправлена в ФБУ"]

                        appFbuReceived:
                        получена информация о подтверждении получения ФБУ запроса (пакета заявок) на регистрацию карт тахографа и выпуск сертификатов.
                        [app::"9. Получена в ФБУ"]

                        appFbuProcessing:
                        получена информация о принятии ФБУ пакета, в который входит заявка, в работу.
                        [app::"10. Взята в работу в ФБУ"]

                        appFbuCompleted (FBURegistered):
                        зарегистрировано в перечнях ФБУ, при этом заполняется список CardInfo, с информацией о номерах карт и
                        началом/окончанием их действиял
                        [app::"11. Исполнена ФБУ"]

                        appFbuRejected (FBUReject):
                        [app::"а) FBUReject"]

                        appFbuRejectedDuplicate (FBUDuplicateOrder):
                        [app::"б) FBUDuplicateOrder"]

                        appMfrSentToCA:
                        [app::"12. Отправлена в УЦ"]

                        appCAReceivedFromMfr
                        [app::"13. Получена в УЦ"]

                        appCADone
                        [app::"14. Исполнена УЦ"]

                        appCARejected
                        [app::"15. Отклонена УЦ"]

                        appMfrPersonalized / appMfrPartiallyPersonalized (Personalized/PersonalizedPart):
                        карты изготовлены, при этом полностью заполняется CardInfo с информацией о изготовленных картах PersonalizedPart - карты частично изготовлены,
                        при этом полностью заполняется CardInfo с информацией о изготовленных картах;
                        [app::"16. Произведена"]

                        appMfrShippedToOvk:
                        отгружено в ОВК
                        [app::"17. Отгружена"]

                        appMfrDone:
                        [app::"18. Исполнена"]

                    </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
                        <xsd:enumeration value="appMfrReceivedFromOvk" />
                        <xsd:enumeration value="appMfrRejected" />
                        <xsd:enumeration value="appMfrRejectedDuplicate" />
                        <xsd:enumeration value="appMfrProcessing" />
                        <xsd:enumeration value="appMfrPaymentCheckPoint" />
                        <xsd:enumeration value="appMfrSentToFbu" />
                        <xsd:enumeration value="appFbuReceived" />
                        <xsd:enumeration value="appFbuProcessing" />
                        <xsd:enumeration value="appFbuCompleted" />
                        <xsd:enumeration value="appFbuRejected" />
                        <xsd:enumeration value="appFbuRejectedDuplicate" />
                        <xsd:enumeration value="appMfrSentToCA" />
                        <xsd:enumeration value="appCAReceivedFromMfr" />
                        <xsd:enumeration value="appCADone" />
                        <xsd:enumeration value="appCARejected" />
                        <xsd:enumeration value="appMfrPersonalized" />
                        <xsd:enumeration value="appMfrPartiallyPersonalized" />
                        <xsd:enumeration value="appMfrShippedToOvk" />
                        <xsd:enumeration value="appMfrDone" />

                        <!-- obsolete statuses that we still support -->

                        <xsd:enumeration value="NotPersonalized" />
                        <xsd:enumeration value="CardManDuplicateOrder" />
                        <xsd:enumeration value="SendFBU" />
                        <xsd:enumeration value="FBURegistered" />
                        <xsd:enumeration value="FBUReject" />
                        <xsd:enumeration value="FBUDuplicateOrder" />
                        <!-- xsd:enumeration value="SendPersonalisation" /> -->
                        <xsd:enumeration value="Personalized" />
                        <xsd:enumeration value="PersonalizedPart" />

                    </xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="CardInfo" type="CardInfoType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						Информация о картах: заполняется ФБУ при
						отправке выписки или производителем карт при
